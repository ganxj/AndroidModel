<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:bind="http://schemas.android.com/apk/res-auto">

    <data>
        <variable
            name="myHead"
            type="com.gange.component.myHead.MyHeadMp"/>
        <variable
            name="mp"
            type="com.gange.packagegroup.model.MyModel"/>

        <variable
            name="item"
            type="com.gange.component.itemEntry.ItemEntryMp"/>

    </data>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <include layout="@layout/component_my_head_vew1"
            bind:mp="@{myHead}"/>

        <include layout="@layout/component_item_entry_view"
            bind:item="@{item}"/>

    </LinearLayout>


</layout>